# Copy this file to .env.docker and fill in your values
# These are used by docker-compose.yml

# Database
POSTGRES_PASSWORD=camino_secure_password_2024

# Redis
REDIS_PASSWORD=redis_secure_password_2024

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-super-secret-nextauth-key-must-be-at-least-32-characters-long

# External API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ASSEMBLYAI_API_KEY=your-assemblyai-api-key-here

# Strava Integration
STRAVA_CLIENT_ID=your-strava-client-id
STRAVA_CLIENT_SECRET=your-strava-client-secret

# Garmin Integration (optional)
GARMIN_CONSUMER_KEY=your-garmin-consumer-key
GARMIN_CONSUMER_SECRET=your-garmin-consumer-secret

# Vercel Blob Storage (if using)
BLOB_READ_WRITE_TOKEN=your-vercel-blob-token

# Application Environment
NODE_ENV=development